/ {
  combos {
    compatible = "zmk,combos";
    
    game_toggle_combo {
      timeout-ms = <2000>;
      key-positions = <32 33 34>;
      bindings = <&to U_GAME>;
      layers = <U_BASE>;
    };
  };
};

